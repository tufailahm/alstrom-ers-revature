 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SignInController</servlet-name>
  	<display-name>SignInController</display-name>
  	<description></description>
  	<servlet-class>com.training.ers.controller.SignInController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignUpController</servlet-name>
  	<display-name>SignUpController</display-name>
  	<description></description>
  	<servlet-class>com.training.ers.controller.SignUpController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignInController</servlet-name>
  	<url-pattern>/SignInController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUpController</servlet-name>
  	<url-pattern>/SignUpController</url-pattern>
  </servlet-mapping>
</web-app>
